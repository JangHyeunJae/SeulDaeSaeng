	<insert id="memberJoin" parameterType="memVO">
		
		INSERT INTO "USERS"(USERS_NO, USERS_ROLE, USERS_ID, USERS_PASS)
		VALUES(#{userNo}, #{userRole}, #{userId}, #{userPass})
	
	</insert>
	
